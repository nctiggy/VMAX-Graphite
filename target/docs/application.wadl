          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/replication/symmetrix/{symmetrixId}/storagegroup">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[the Symmetrix that the request is being made to.  This is the 12 digit Symmetrix ID.]]>
