          <wadl:param name="force" style="query"/>
          <wadl:param name="symforce" style="query"/>
          <wadl:param name="star" style="query"/>
        </wadl:request>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/84/replication/symmetrix/{symmetrixId}/storagegroup/{storageGroupId}/snapshot/{snapshotId}/generation/{generationNumber}">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[the Symmetrix that the Storage Group is on.  This is the 12 digit Symmetrix ID.]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="storageGroupId" style="template">
        <wadl:doc>
          <![CDATA[the Storage Group name for the group being looked for]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="snapshotId" style="template">
        <wadl:doc>
          <![CDATA[the snapshot name being acted upon]]>
        </wadl:doc>
      </wadl:param>
      <wadl:param name="generationNumber" style="template">
        <wadl:doc>
          <![CDATA[the snapshot generation being acted upon]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[This call gets the generation details for a Storage Group snapshot generation number.]]>
        </wadl:doc>
        <wadl:request/>
        <wadl:response>
          <wadl:doc>
            <![CDATA[the object containing the details of the snapshot generation]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="replication84:snapVXSnapshotGeneration" xmlns:replication84="http://www.emc.com/em/2012/07/univmax/restapi/84/replication"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="PUT">
        <wadl:request>
          <wadl:representation mediaType="application/xml" element="replication84:storageGroupSnapshotGenUpdate" xmlns:replication84="http://www.emc.com/em/2012/07/univmax/restapi/84/replication"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:representation mediaType="application/xml"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
      <wadl:method name="DELETE">
        <wadl:doc>
          <![CDATA[This call terminates a snapshot generation.  If the snapshot generation is restored, the restored
session will be terminated.  If there is no restore session associated with the snapshot generation,
the generation itself will be terminated.  If it is not known if the generation is restored before
issuing the DELETE command, performing a GET for the generations after a successful DELETE will indicate
if the generation still exists (a restore was terminated) or if the generation is gone (the original
generation was terminated).]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="force" style="query"/>
          <wadl:param name="symforce" style="query"/>
          <wadl:param name="star" style="query"/>
        </wadl:request>
      </wadl:method>
    </wadl:resource>
