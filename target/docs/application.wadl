            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[the object containing the list of Storage Group names]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="replication:replicationSymmetrixStorageGroupList" xmlns:replication="http://www.emc.com/em/2012/07/univmax/restapi/replication"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/sloprovisioning/symmetrix/{symmetrixId}/director">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
