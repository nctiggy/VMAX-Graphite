            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group volumes ids that are reserved (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="pinned" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group volumes ids that are pinned  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="encapsulated" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group volumes ids that are encapsulated  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="associated" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group volumes ids that are associated  (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="raid1" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group volumes ids that are raid1 (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="raid5" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group volumes ids that are raid5 (true/false)]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="raid6" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters returned list to display
Storage Group volumes ids that are raid6  (true/false)]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[A session based Iterator which has 1000 volume Ids per iteration, expires after 10 minutes,
and can be queried from common resource using URL: /common/Iterator/{iteratorId}, alternatively an empty list iterator.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="common:iterator" xmlns:common="http://www.emc.com/em/2012/07/univmax/restapi/common"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/replication/devicegroup/{deviceGroupId}/rdf_group">
      <wadl:param name="deviceGroupId" style="template">
