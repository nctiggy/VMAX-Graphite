    <wadl:resource path="/restapi/wlp/symmetrix/{symmetrixId}/headroom">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Get headroom capacity values for all SLO/WorkloadType combinations on all SRPs on a given Symmetrix array, Headroom expiry with confidence > 0.5 will also be returned.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="srp" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters the returned list to only show specified SRP Headroom details]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="slo" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters the returned list to only show specified slo Headroom details]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="workloadtype" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters the returned list to only show specified workloadtype Headroom details [NONE,OLTP,OLTP_REP,DSS,DSS_REP]]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="headroom_expiry" style="query">
            <wadl:doc>
              <![CDATA[Optional value that forces Headroom expiry with confidence <0.5 to be displayed (true/false)]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[List of headroom capacity values and last updated timestamps.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp:headroomResult" xmlns:wlp="http://www.emc.com/em/2012/07/univmax/restapi/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
