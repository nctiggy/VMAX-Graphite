          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="common:genericResult" xmlns:common="http://www.emc.com/em/2012/07/univmax/restapi/common"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
