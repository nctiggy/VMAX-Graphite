        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="POST">
        <wadl:doc>
          <![CDATA[Generate admissibility scores and recommended front-end ports for a given workload or workloads.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:doc>
            <![CDATA[Contains one or more full workload characterizations (including mixture and skew information)]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp:symWorkloadAdmissibilityParam" xmlns:wlp="http://www.emc.com/em/2012/07/univmax/restapi/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[Admissibility scores for several array components (FE, BE, Cache, SRDF) and sorted list of ports.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp82:admissibilityResult" xmlns:wlp82="http://www.emc.com/em/2012/07/univmax/restapi/82/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/82/wlp/symmetrix/{symmetrixId}/compliance">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Get a list of storage groups and the percentage of time spent in each SLO compliance state for a given time window along with any epoch and/or excluded bucket ids set.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="starttime" style="query">
            <wadl:doc>
              <![CDATA[the start time in milliseconds]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="endtime" style="query">
            <wadl:doc>
              <![CDATA[the end time in milliseconds]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[the storage group name, percent of time spent in each state (stable, marginal and critical), epoch and/or excluded bucket ids set for the system]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp82:sloComplianceResult" xmlns:wlp82="http://www.emc.com/em/2012/07/univmax/restapi/82/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
    <wadl:resource path="/restapi/82/wlp/symmetrix/{symmetrixId}/headroom">
      <wadl:param name="symmetrixId" style="template">
        <wadl:doc>
          <![CDATA[The Symmetrix Array ID]]>
        </wadl:doc>
      </wadl:param>
      <wadl:method name="GET">
        <wadl:doc>
          <![CDATA[Get headroom capacity values for all SLO/WorkloadType combinations on all SRPs on a given Symmetrix array, Headroom expiry with confidence > 0.5 will also be returned.]]>
        </wadl:doc>
        <wadl:request>
          <wadl:param name="srp" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters the returned list to only show specified SRP Headroom details]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="slo" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters the returned list to only show specified slo Headroom details]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="workloadtype" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters the returned list to only show specified workloadtype Headroom details [NONE,OLTP,OLTP_REP,DSS,DSS_REP]]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="emulation" style="query">
            <wadl:doc>
              <![CDATA[Optional value that filters the returned list to only show headroom for a specific emulation for an SRP [FBA, CKD]]]>
            </wadl:doc>
          </wadl:param>
          <wadl:param name="headroom_expiry" style="query">
            <wadl:doc>
              <![CDATA[Optional value that forces Headroom expiry with confidence <0.5 to be displayed (true/false)]]>
            </wadl:doc>
          </wadl:param>
        </wadl:request>
        <wadl:response>
          <wadl:doc>
            <![CDATA[List of headroom capacity values and last updated timestamps.]]>
          </wadl:doc>
          <wadl:representation mediaType="application/xml" element="wlp82:wlpHeadroomResult" xmlns:wlp82="http://www.emc.com/em/2012/07/univmax/restapi/82/wlp"/>
          <wadl:representation mediaType="application/x-java-serialized-object"/>
          <wadl:representation mediaType="application/json"/>
        </wadl:response>
      </wadl:method>
    </wadl:resource>
